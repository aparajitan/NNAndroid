                        android:textSize="16sp" />

                    <LinearLayout
                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <LinearLayout
                            android:layout_marginLeft="@dimen/post_property_form_left_right_padding"
                            android:layout_marginRight="@dimen/post_property_form_left_right_padding"
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <FrameLayout
                                android:layout_width="wrap_content"
                                android:background="@drawable/post_unselect_background"
                                android:layout_height="@dimen/_30sdp">
                                <TextView
                                    android:layout_gravity="center_vertical"
                                    android:layout_width="wrap_content"
                                    android:textSize="14sp"
                                    android:fontFamily="@font/montserrat_regular"
                                    android:layout_marginRight="@dimen/_10sdp"
                                    android:layout_marginLeft="@dimen/_10sdp"
                                    android:textColor="@color/text_color"
                                    android:layout_height="wrap_content"
                                    android:text="Yes"/>
                            </FrameLayout>
                            <FrameLayout
                                android:layout_width="wrap_content"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/post_unselect_background"
                                android:layout_height="@dimen/_30sdp">
                                <TextView
                                    android:layout_gravity="center_vertical"
                                    android:layout_width="wrap_content"
                                    android:textSize="14sp"
                                    android:layout_marginRight="@dimen/_10sdp"
                                    android:layout_marginLeft="@dimen/_10sdp"
                                    android:fontFamily="@font/montserrat_regular"
                                    android:textColor="@color/text_color"
                                    android:layout_height="wrap_content"
                                    android:text="No" />
                            </FrameLayout>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <TextView
                    android:layout_marginTop="@dimen/_15sdp"
                    android:background="@color/line_color"
                    android:layout_width="match_parent"

                    android:layout_height="1dp"/>


                <FrameLayout
                    android:gravity="center"
                    android:layout_marginBottom="@dimen/_10sdp"
                    android:layout_marginTop="15dp"
                    android:background="@drawable/button_back"
                    android:id="@+id/upload_id"
                    android:visibility="visible"
                    android:layout_marginLeft="@dimen/_45sdp"
                    android:layout_marginRight="@dimen/_45sdp"
                    android:layout_gravity="center_horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/button_height">
                    <TextView
                        android:layout_width="wrap_content"
                        android:text="View 40 Properties"
                        android:layout_gravity="center"
                        android:textColor="@color/button_text_color"
                        android:textSize="@dimen/button_size"
                        android:fontFamily="@font/montserrat_regular"
                        android:layout_height="wrap_content"/>
                </FrameLayout>

            </LinearLayout>

        </LinearLayout>



    </ScrollView>

